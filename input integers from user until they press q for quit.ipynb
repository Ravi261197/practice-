    "sum=0\n",
    "prod=1\n",
    "print(\"press q if your inputinng is done\")\n",
    "while True:\n",
    "    i=int(input(\"start inputing number \"))\n",
    "    sum=sum+i\n",
    "    prod=prod*i\n",
    "    quit=str(input())\n",
    "    if quit=='q':\n",
    "        break\n",
    "print(\"sum is \",sum,\"product is \",prod)"
   ]
